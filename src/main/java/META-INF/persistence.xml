<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">

    <class>entity.ClazzEntity</class>
    <class>entity.CourseEntity</class>
    <class>entity.CourseregistrationsessionEntity</class>
    <class>entity.SemesterEntity</class>
    <class>entity.StudentEntity</class>
    <class>entity.StudentregistercourseEntity</class>
    <class>entity.SubjectEntity</class>
    <class>entity.TeacherEntity</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/qlkh"/>
      <property name="hibernate.connection.driver_class" value="com.mysql.cj.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
